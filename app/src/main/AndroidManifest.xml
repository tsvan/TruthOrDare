<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tsvan.com">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GameSliderActivity"></activity>
        <activity android:name=".SelectPackageActivity" />
        <activity android:name=".AddQuestionsActivity" />
        <activity android:name=".EditPackageActivity" />
        <activity
            android:name=".AddPackageActivity"
            android:label="@string/title_activity_add_package"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AddQuestionActivity"
            android:label="@string/title_activity_add_questions"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>